{
  "name": "@brightcove/player-url",
  "version": "1.0.0",
  "description": "A module for getting a URL to a Brightcove Player.",
  "main": "dist/brightcove-player-url.cjs.js",
  "module": "dist/brightcove-player-url.es.js",
  "browserslist": [
    "defaults",
    "ie 11"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:js": "rollup -c scripts/rollup.config.js",
    "clean": "rimraf dist",
    "postclean": "mkdirp dist",
    "lint": "vjsstandard",
    "pretest": "npm-run-all lint build",
    "test": "tap test/index.test.js",
    "preversion": "npm test",
    "version": "node scripts/version.js",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "npm run build:js -- -w",
    "prepublish": "not-in-install && npm run build || in-install",
    "prepush": "npm run lint"
  },
  "keywords": [],
  "author": "Brightcove, Inc.",
  "license": "Apache-2.0",
  "vjsstandard": {
    "ignore": [
      "dist"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "conventional-changelog-cli": "^2.0.0",
    "conventional-changelog-videojs": "^3.0.0",
    "husky": "^0.14.3",
    "in-publish": "^2.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.63.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "semver": "^5.1.0",
    "tap": "^12.0.1",
    "tsml": "^1.0.1",
    "uglify-es": "^3.3.9",
    "videojs-standard": "^6.0.3"
  }
}
